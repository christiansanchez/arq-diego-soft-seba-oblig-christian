<?xml version="1.0" encoding="UTF-8"?>
<service>
	<description> Web service que permite saludar y despedir.
	</description>
	<messageReceivers>
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out"
			class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</messageReceivers>
	<parameter name="ServiceClass">MiWebService
	</parameter>
	<operation name="saludar" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="despedir" mep="http://www.w3.org/2004/08/wsdl/in-out" />
</service>
