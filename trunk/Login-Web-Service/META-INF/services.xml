<?xml version="1.0" encoding="UTF-8"?>
<service>
	<description> 
		Web service login.
	</description>
	<messageReceivers>
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out"
		class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</messageReceivers>
	<parameter name="ServiceClass">
		ServicioLogin
	</parameter>
	<operation name="existsUser" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="createUser" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="removeUser" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="validateUser" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="listUsers" mep="http://www.w3.org/2004/08/wsdl/in-out" />
</service>